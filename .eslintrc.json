{
  "parser": "babel-eslint",
  "extends": "airbnb-base",
  "rules": {
    "quotes": [2, "double", { "avoidEscape": false }],
    "new-cap": 0,
    "default-case": 0,
    "no-shadow": 0,
    "no-cond-assign": [2, "except-parens"],
    "no-param-reassign": [2, { "props": false }],
    "no-underscore-dangle": [2, { "allow": ["__"] }]
  }
}
